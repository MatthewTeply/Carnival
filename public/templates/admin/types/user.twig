{% if value %}
    <div class="type-user-container">
        <a href="{{ path(value|getClass|split('Entity\\')[1]) }}/show?id={{ value.id }}" carnival-fade="false">
            <img src="{{ __webStorage__ ~ value.img.relativePath }}" alt=""> {{ value.username }}
        </a>
    </div>
{% else %}
    <div class="badge badge-light">NULL</div>
{% endif %}